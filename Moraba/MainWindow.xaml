<Window x:Class="Moraba.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Moraba"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="startButton" Content="Start" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="startButton_Click"/>
        <TextBox x:Name="player2box" HorizontalAlignment="Left" Height="25" Margin="257,11,0,0" TextWrapping="Wrap" Text="Player 2" VerticalAlignment="Top" Width="139" IsEnabled="False"/>
        <TextBox x:Name="Player1Box" HorizontalAlignment="Left" Height="25" Margin="104,11,0,0" TextWrapping="Wrap" Text="Player 1" VerticalAlignment="Top" Width="139" IsEnabled="False"/>
        <Button x:Name="finish" Content="Finish" HorizontalAlignment="Left" Margin="415,11,0,0" VerticalAlignment="Top" Width="75" Click="finish_Click"/>

    </Grid>
</Window>
